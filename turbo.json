{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build/**", ".next/**"],
            "cache": true
        },
        "dev": {
            "cache": false
        },
        "lint": {
            "outputs": []
        },
        "migrate": {
            "outputs": []
        },
        "test": {
            "outputs": []
        }
    }
}
